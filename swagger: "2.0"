        type: "integer"
      responses:
        200:
          description: OK
        400:
          description: "Invalid id supplied"
        404:
          description: "Id not found"
    put:
      tags: 
      - "job"
      summary: "create subjob for {jobId}"
      description: ""
      operationId: "createSubJob"
      produces:
      - "application/xml"
      - "application/json"
      parameters:
      - name: "jobId"
        in: "path"
        description: ""
        required: true
        type: "integer"
        format: "int64"
      - name: "subjobId"
        in: "path"
        description: ""
        required: true
        type: "integer"
        format: "int64"
      - in: "body"
        name: "body"
        description: "created new subjob object"
        required: true
        schema:
          $ref: "#/definitions/SubJobForAJob"
      responses:
        default:
